{
    "name": "mpancaldi",
    "version": "0.0.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port 4203",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "jest",
        "lint": "ng lint",
        "lint:fix": "prettier --write src && ng lint --fix",
        "firebase:serve": "firebase serve --only hosting",
        "firebase:deploy": "firebase deploy --only hosting:martapancaldi",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged --relative",
            "pre-push": "lint-staged --relative"
        }
    },
    "lint-staged": {
        "src/**/*.{js,ts,html,css,scss}": [
            "prettier --write && eslint"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular-builders/jest": "^15.0.0",
        "@angular/animations": "^15.1.0",
        "@angular/cdk": "15.1.2",
        "@angular/common": "^15.1.0",
        "@angular/compiler": "^15.1.0",
        "@angular/core": "^15.1.0",
        "@angular/forms": "^15.1.0",
        "@angular/material": "15.1.2",
        "@angular/platform-browser": "^15.1.0",
        "@angular/platform-browser-dynamic": "^15.1.0",
        "@angular/router": "^15.1.0",
        "@fortawesome/angular-fontawesome": "^0.12.1",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/free-brands-svg-icons": "^6.2.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.1",
        "@martapanc/angular2-image-gallery": "15.2.0",
        "@ng-bootstrap/ng-bootstrap": "^14.0.1",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "@types/jest": "^29.4.0",
        "bootstrap": "^5.2.3",
        "firebase": "^9.16.0",
        "firebase-tools": "^11.21.0",
        "github-calendar": "^2.3.2",
        "hammerjs": "^2.0.8",
        "jest": "^29.4.1",
        "jest-preset-angular": "^12.2.6",
        "rxjs": "~7.8.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.3.0",
        "zone.js": "^0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.1.3",
        "@angular-eslint/builder": "15.2.0",
        "@angular-eslint/eslint-plugin": "15.2.0",
        "@angular-eslint/eslint-plugin-template": "15.2.0",
        "@angular-eslint/schematics": "15.2.0",
        "@angular-eslint/template-parser": "15.2.0",
        "@angular/cli": "~15.1.3",
        "@angular/compiler-cli": "^15.1.0",
        "@types/jasmine": "~4.3.0",
        "@typescript-eslint/eslint-plugin": "5.48.1",
        "@typescript-eslint/parser": "5.48.1",
        "app-root-path": "^3.1.0",
        "eslint": "^8.31.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "gm": "^1.25.0",
        "husky": "^8.0.0",
        "jasmine-core": "^4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "lint-staged": "^13.1.0",
        "mkdirp": "^2.1.3",
        "onecolor": "^4.0.0",
        "prettier": "^2.8.3",
        "prettier-eslint": "^15.0.1",
        "typescript": "^4.9.4"
    }
}
